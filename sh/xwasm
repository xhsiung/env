alias @wasm-lib_nodejs="rm -rf mywasmnodejs; mkdir mywasmnodejs; cd mywasmnodejs;
                        cp -a ~/.env/tpl/wasm-pack/notpl/* .;
                        echo 'create wasm-lib_nodejs project';
                        "

alias @wasm-lib2_nodejs="rm -rf mywasmnodejs; mkdir mywasmnodejs;cd mywasmnodejs;
                        cp -a ~/.env/tpl/wasm-pack/notpl2/* .;
                        echo 'create wasm-lib2_nodejs project';
                        "

alias @wasm-pack="rm -rf mywasmpack; mkdir mywasmpack; cd mywasmpack;
                        cp -a ~/.env/tpl/wasm-pack/tpl/* .;
                        echo 'create wasm-pack project';
                        "

alias @wasm-pack2="rm -rf mywasmpack; mkdir mywasmpack; cd mywasmpack;
                        cp -a ~/.env/tpl/wasm-pack/tpl2/* .;
                        echo 'create wasm-pack2 project';
                        "

alias @wasm-pack3="rm -rf mywasmpack; mkdir mywasmpack; cd mywasmpack;
                        cp -a ~/.env/tpl/wasm-pack/tpl3/* .;
                        echo 'create wasm-pack3 project';
                        "

alias @wasm-wasmtime="cargo new mywasmtime; cd mywasmtime;
                        cp -a ~/.env/tpl/wasmtime/* .;
                        echo 'create wasmtime project';
                        "

alias @wasm-wasmer="rm -rf mywasmer;mkdir mywasmer;cd mywasmer;
                        cp -a ~/.env/tpl/wasmer/* .;
                        echo 'create wasmer project';
                        "

