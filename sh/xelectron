alias @electron-proj="rm -rf myelectron;mkdir myelectron;cd myelectron;
                        cp -a ~/.env/tpl/electron/* .;
                        echo 'create myelectron project';
                        "

alias @electron-nodepy="rm -rf myelectron;mkdir myelectron;cd myelectron;
                        cp -a ~/.env/tpl/electron-nodepy/* .;
                        echo 'create myelectron-nodepy project';
                        "

alias @electron-express="rm -rf myelectron;mkdir myelectron;cd myelectron;
                        cp -a ~/.env/tpl/electron-express/* .;
                        echo 'create myelectron-express project';
                        "

alias @electron-win="rm -rf myelectron;mkdir myelectron;cd myelectron;
                        cp -a ~/.env/tpl/electron-win/* .;
                        echo 'create myelectron-win project';
                        "
