alias @gitBranch="storecmd 'cmd@git branch'"
alias @gitBranchAdd="storecmd 'cmd@git branch \$addBranch'"
alias @gitBranchDel="storecmd 'cmd@git branch -d \$delBranch'"
alias @gitBranchNewCheckout="storecmd 'cmd@git checkout -b \$branch'"

alias @gitCommTag="storecmd 'cmd@git commit -m \"add detail\"' \
			    'cmd@git log --oneline' \
  			    'cmd@git tag \$tag100 3d996b3' \
			    'cmd@git push \$Branch \$tagName'"  

alias @gitCommBranch="storecmd 'cmd@git tag \$tag100' \
			       'cmd@git commit -a -m \"add main\"'"

alias @gitCheckoutSwitch="storecmd 'cmd@git branch' \
				   'cmd@git checkout \$branch'"

alias @gitCheckoutTag="storecmd 'cmd@git checkout tags/\$mytags'"
alias @gitMerge="storecmd 'cmd@git branch' \
			  'cmd@git checkout \$branch' \
			  'cmd@git merge \$otherBranch'"

alias @gitRemote="storecmd 'cmd@git remote add tsmc-djweb http://axsoho.ddns.net:6610/projects/tsmc-djweb' \
			   'cmd@git push tsmc-djweb'"
